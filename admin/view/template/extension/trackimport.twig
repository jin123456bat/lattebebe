{{ header }}{{ column_left }}
<div id="content">
 <div class="page-header">
	<div class="container-fluid">
		<div class="pull-right">
			<button type="submit" form="form-import" data-toggle="tooltip" title="Import" class="btn btn-primary"><i class="fa fa-upload"></i></button>
			<a href="{{ cancel }}" data-toggle="tooltip" title="Cancel" class="btn btn-default"><i class="fa fa-reply"></i></a>
		</div>
		<h1>{{ heading_title }}</h1>
		<ul class="breadcrumb">
		{% for breadcrumb in breadcrumbs %}
			<li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
		{% endfor %}
		</ul>
	</div>
 </div>
 <div class="container-fluid">
	<div class="panel panel-default">
	  <div class="panel-heading">
		<h3 class="panel-title"><i class="fa fa-pencil"></i> {{ heading_title }}</h3>
	  </div>
	  <div class="panel-body">
		  <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-import" class="form-horizontal">
			<div class="form-group">
			  <label class="col-sm-2 control-label">{{ entry_orderstatus }}</label>
			  <div class="col-sm-10">
				<select name="order_status_id" id="input-order-status" class="form-control">
                  <option value="*"></option>
                  {% for order_status in order_statuses %}
					  {% if order_status['order_status_id'] == filter_order_status %}
                  <option value="{{ order_status['order_status_id'] }}" selected="selected">{{ order_status['name'] }}</option>
                 {% else %}
                  <option value="{{ order_status['order_status_id'] }}">{{ order_status['name'] }}</option>
                  {% endif %}
                  {% endfor %}
                </select>
			  </div>
			</div>
			<div class="form-group">
			  <label class="col-sm-2 control-label">{{ entry_import }}</label>
			  <div class="col-sm-10">
				<input style="height:46px;" type="file" name="import" value=""/>
			  </div>
			</div>
		  </form>
	  </div>
	</div>
 </div>
</div>
{{ footer }}